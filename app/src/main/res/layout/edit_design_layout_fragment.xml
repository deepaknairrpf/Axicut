<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.dingu.axicut.Design.EditDesignLayout">

    <!-- TODO: Update blank fragment layout -->
  <RelativeLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_centerInParent="true">
    <EditText
        android:id="@+id/designLayoutEditText"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:hint="Enter design layout"/>
    <Button
        android:id="@+id/workOrderSaveButton"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Save"
        android:layout_below="@+id/designLayoutEditText"
        android:layout_centerHorizontal="true"/>

  </RelativeLayout>

</RelativeLayout>
